<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.FunctionActivity">

    <Button
        android:id="@+id/trackExpensesBtn"
        android:layout_width="203dp"
        android:layout_height="0dp"
        android:layout_marginTop="205dp"
        android:backgroundTint="#BE65CF"
        android:text="@string/trackExpensesBtn"
        android:textSize="20sp"
        app:icon="@drawable/baseline_attach_money_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/diaryBtn"
        android:layout_width="202dp"
        android:layout_height="0dp"
        android:layout_marginBottom="289dp"
        android:backgroundTint="#BE65CF"
        android:text="@string/diaryBtn"
        android:textSize="20sp"
        app:icon="@drawable/baseline_book_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>